{
  "expo": {
    "name": "ShopMore",
    "slug": "shopmore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon.png",
    "splash": {
      "image": "./app/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./app/assets/images/favicon.png"
    },
    "description": "An intuitive and design-simple React Native E-Commerce application for selling products online",
    "githubUrl": "https://github.com/shanematthewkelly/ShopMore"
  }
}
